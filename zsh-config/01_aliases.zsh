# ~/.zsh-config/01_aliases.zsh
# 存放所有别名

alias zshconfig="code ~/.zsh-config"
alias zshrc="code ~/.zshrc"
alias zshenv="code ~/.zshenv"

# ls
alias l='ls -lAh'
alias ll='ls -lAh'
alias la='ls -A'
alias lsa='ls -la'

# Git
alias g='git'
alias ga='git add'
alias gc='git commit -m'
alias gs='git status'
alias gco='git checkout'
alias gb='git branch'

# Docker
alias dps='docker ps'
